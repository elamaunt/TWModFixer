<Window x:Class="TWModFixer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TWModFixer"
        mc:Ignorable="d"
        Title="MainWindow" Height="197" Width="489">
    <Grid Margin="0,10,0,0">
        <TextBlock x:Name="YourFile" HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" Text="Etalon file" VerticalAlignment="Top" Width="381" Height="50"/>
        <TextBlock x:Name="OtherFile" HorizontalAlignment="Left" Margin="10,55,0,0" TextWrapping="Wrap" Text="Need fixed file" VerticalAlignment="Top" Width="381" Height="50"/>
        <Button x:Name="SetYour" Click="SetYour_Click" Content="Set" HorizontalAlignment="Left" Margin="396,0,0,0" VerticalAlignment="Top" Width="75" Height="20"/>
        <Button x:Name="SetOther" Click="SetOther_Click" Content="Set" HorizontalAlignment="Left" Margin="396,55,0,0" VerticalAlignment="Top" Width="75" Height="20"/>
        <Button x:Name="Check" Click="Check_Click" Content="Check and fix" HorizontalAlignment="Left" Margin="10,126,0,0" VerticalAlignment="Top" Width="461" Height="20"/>
    </Grid>
</Window>
